{
  "name": "dse-static-playwright",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint:check": "eslint . --cache --ext .js,.ts --ignore-path .gitignore",
    "format:check": "prettier . \"!./html/**\" --cache --check --ext .js,.ts --ignore-path .gitignore",
    "compile-scss": "rm -f html/css/* && sass --update --style=expanded scss:html/css",
    "build": "pnpm run compile-scss && vite build -l info",
    "e2e:run_all_tests": "npx playwright test --project=chromium",
    "e2e:run_string_match_edition_view": "npx playwright test e2e/specs/edition_string_match.spec.ts --project=chromium",
    "e2e:edition_view": "npx playwright test e2e/specs/edition_view.spec.ts --project=chromium",
    "e2e:edition_view_update_snapshots": "npx playwright test e2e/specs/edition_view.spec.ts --update-snapshots --project=chromium"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "5.3.3",
    "de-micro-editor": "^0.4.2",
    "instantsearch.js": "^4.81.0",
    "mark.js": "^8.11.1",
    "mirador": "3.4.3",
    "openseadragon": "^5.0.1",
    "typesense-instantsearch-adapter": "^2.9.0"
  },
  "devDependencies": {
    "@acdh-oeaw/eslint-config": "^2.0.10",
    "@acdh-oeaw/eslint-config-playwright": "^1.0.10",
    "@acdh-oeaw/prettier-config": "^2.0.1",
    "@playwright/test": "^1.56.1",
    "@types/bootstrap": "^5.2.10",
    "@types/node": "^20.19.24",
    "@types/openseadragon": "^3.0.10",
    "ci-info": "^4.3.1",
    "glob": "^11.0.3",
    "sass": "^1.93.2",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  },
  "eslintConfig": {
    "extends": [
      "@acdh-oeaw/eslint-config",
      "@acdh-oeaw/eslint-config/strict",
      "@acdh-oeaw/eslint-config-playwright"
    ]
  },
  "prettier": "@acdh-oeaw/prettier-config",
  "type": "module",
  "pnpm": {
    "overrides": {
      "braces@<3.0.3": ">=3.0.3",
      "micromatch@<4.0.8": ">=4.0.8",
      "vite@>=5.3.0 <5.3.6": ">=5.3.6",
      "vite@>=5.3.0 <=5.3.5": ">=5.3.6",
      "rollup@>=4.0.0 <4.22.4": ">=4.22.4",
      "axios@>=1.0.0 <1.8.2": ">=1.8.2",
      "@babel/runtime@<7.26.10": ">=7.26.10",
      "cross-spawn@>=7.0.0 <7.0.5": ">=7.0.5",
      "brace-expansion@>=1.0.0 <=1.1.11": ">=1.1.12",
      "brace-expansion@>=2.0.0 <=2.0.1": ">=2.0.2",
      "dompurify@<3.2.4": ">=3.2.4",
      "form-data@>=4.0.0 <4.0.4": ">=4.0.4",
      "glob@>=11.0.0 <11.1.0": ">=11.1.0",
      "preact@>=10.27.0 <10.27.3": ">=10.27.3",
      "lodash@>=4.0.0 <=4.17.22": ">=4.17.23",
      "js-yaml@>=4.0.0 <4.1.1": ">=4.1.1",
      "@isaacs/brace-expansion@<=5.0.0": ">=5.0.1",
      "axios@<=1.13.4": ">=1.13.5",
      "qs@<6.14.1": ">=6.14.1"
    }
  }
}